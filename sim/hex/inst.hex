// 一些简单的RISC-V指令，每行一条32位指令
// 格式：8位十六进制数（对应32位指令）
// 示例程序：
// addi x1, x0, 5    // x1 = 5
// addi x2, x0, 3    // x2 = 3
// sub  x3, x1, x2   // x3 = x1 - x2 = 2
// sw   x3, 4(x0)    // mem[4] = x3
// lw   x4, 4(x0)    // x4 = mem[4]

00500093  // addi x1, x0, 5
00300113  // addi x2, x0, 3
40208133  // sub  x3, x1, x2
00302223  // sw   x3, 4(x0)
00402203  // lw   x4, 4(x0)
00000000  // nop
00000000  // nop
00000000  // nop 