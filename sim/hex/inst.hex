// 一些简单的RISC-V指令，每行一条32位指令
// 格式：8位十六进制数（对应32位指令）
// 示例程序：
// addi x1, x0, 1    // x1 = 1
// addi x2, x0, 2    // x2 = 2
// add  x3, x1, x2   // x3 = x1 + x2
// sw   x3, 0(x0)    // mem[0] = x3
// lw   x4, 0(x0)    // x4 = mem[0]

00100093  // addi x1, x0, 1
00200113  // addi x2, x0, 2
002081b3  // add  x3, x1, x2
00302023  // sw   x3, 0(x0)
00002203  // lw   x4, 0(x0)
00000000  // nop
00000000  // nop
00000000  // nop 