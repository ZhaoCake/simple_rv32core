// 一些简单的RISC-V指令，每行一条32位指令
// 格式：8位十六进制数（对应32位指令）
// 示例程序：
// addi x1, x0, 5    // x1 = 5
// addi x2, x0, 3    // x2 = 3
// sb   x1, 1(x0)    // mem[1] = x1[7:0]
// sh   x2, 2(x0)    // mem[2] = x2[15:0]
// lb   x3, 1(x0)    // x3 = sext(mem[1][7:0])
// lh   x4, 2(x0)    // x4 = sext(mem[2][15:0])
// lbu  x5, 1(x0)    // x5 = zext(mem[1][7:0])
// lhu  x6, 2(x0)    // x6 = zext(mem[2][15:0])
// ecall             // 系统调用
// ebreak            // 断点

00500093  // addi x1, x0, 5
00300113  // addi x2, x0, 3
00100023  // sb   x1, 1(x0)
00201123  // sh   x2, 2(x0)
00100183  // lb   x3, 1(x0)
00201203  // lh   x4, 2(x0)
00104283  // lbu  x5, 1(x0)
00205303  // lhu  x6, 2(x0)
00000073  // ecall
00100073  // ebreak
00000000  // nop
00000000  // nop 